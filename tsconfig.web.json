{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "Renderer/*": ["src/renderer/*"],
      "Components/*": ["src/renderer/components/*"],
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    }
  }
}
